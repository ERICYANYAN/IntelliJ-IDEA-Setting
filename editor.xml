<application>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
  </component>
  <component name="OCCodeFoldingSettings">
    <option name="collapseBlockExpressions" value="true" />
    <option name="collapseMultilineComments" value="true" />
    <option name="COLLAPSE_MULTILINE_COMMENTS" value="true" />
    <option name="COLLAPSE_BLOCK_EXPRESSIONS" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>